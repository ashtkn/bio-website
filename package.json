    "gatsby-transformer-sharp": "^2.5.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-spring": "^8.0.27",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-system": "^5.1.10",
    "@types/theme-ui": "^0.3.7",
