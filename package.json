  "author": "Asahi Takenouchi <le-soleil-levant.0802@outlook.com>",
  "description": "This is the website of Asahi Takenouchi",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "clean": "gatsby clean",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "format": "prettier --write '**/*.{js,ts,tsx,html,json,md}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "npm run lint -- --fix"
    ],
    "**/*.{js,ts,tsx,html,json,md}": [
      "npm run format"
    ]
  },
