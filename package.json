  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint --fix"
    ],
    "*.{md,mdx,json,yaml}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
