  "description": "This is the website of Asahi Takenouchi",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "clean": "gatsby clean",
    "tsc": "tsc",
    "type-check": "tsc --noEmit",
